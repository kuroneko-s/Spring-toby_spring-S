<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
        "http://castor.org/mapping.dtd">
<!--Deprecated 당해서 설정 파일만 공시-->
<mapping>
    <class name="org.choidh.toby_project.domain.dao.xjc.Sqlmap">
        <map-to xml="sqlmap"/>
        <field name="sql" type="org.choidh.toby_project.domain.dao.xjc.SqlType"
               required="true" collection="arraylist">
            <bind-xml name="sql" node="element"/>
        </field>
    </class>

    <class name="org.choidh.toby_project.domain.dao.xjc.SqlType">
        <map-to xml="sql"/>
        <field name="key" type="string" required="true">
            <bind-xml name="key" node="attribute"/>
        </field>
        <field name="value" type="string" required="true">
            <bind-xml node="text"/>
        </field>
    </class>
</mapping>
